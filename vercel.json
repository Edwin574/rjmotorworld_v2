{
  "framework": null,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "rewrites": [
    { "source": "/api/:match*", "destination": "/api/index" }
  ],
  "functions": {
    "api/index.ts": {
      "includeFiles": ["server/**", "shared/**"]
    }
  },
  "cleanUrls": true
}


